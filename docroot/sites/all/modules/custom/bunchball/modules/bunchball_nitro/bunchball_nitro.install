<?php

/**
 * @file
 *    Install / uninstall actions.
 */

/**
 * Implement hook_uninstall()
 *
 * Delete all associated variables.
 */
function bunchball_nitro_uninstall() {
  variable_del('bunchball_nitro_admin');
  variable_del('bunchball_nitro_admin_youtube');
  variable_del('bunchball_nitro_notification_interval');
}
